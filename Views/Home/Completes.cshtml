@model Product

@{
    ViewData["Title"] = "Completes";
  
}

<partial name="_Header"></partial>

<h3 class="text-center mb-2">Completes</h3>

<div class="row">

  @foreach (Product prod in ViewBag.AllCompletes)
{

     <div class="col-lg-4 col-md-6 mb-4">
            <div class="card h-100">
              <a href="#"><img class="card-img-top" src="@prod.Photo" alt="@prod.Name"></a>
              <div class="card-body">
                <h4 class="card-title">
                  <a href="/product/@prod.ProductId">@prod.Name</a>
                </h4>
                <h5>$@prod.Price</h5>
                <p class="card-text"></p>
              </div>
              <div class="card-footer">
               
                  
                 
                
                  <a class="btn btn-success" href="/addcart/@prod.ProductId">Add to Cart</a>
                  
               

              </div>
            </div>
          </div>

  
}


</div>
