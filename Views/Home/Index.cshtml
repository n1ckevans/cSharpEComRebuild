	<section class="banner bgwhite p-t-40 p-b-40">
		<div class="container">
      <div class="sec-title p-b-60">
				<h3 class="m-text5 t-center">
					Skateboards
				</h3>
			</div>
			<div class="row">
				<div class="col-sm-10 col-md-8 col-lg-4 m-l-r-auto">

					<div class="block1 hov-img-zoom pos-relative m-b-30">
						<img src="~/img/ah.jpeg" alt="IMG-BENNER">

						<div class="block1-wrapbtn w-size2">
	
							<a href="/decks" class="flex-c-m size2 m-text2 bg3 hov1 trans-0-4">
								Decks
							</a>
						</div>
					</div>

	
					<div class="block1 hov-img-zoom pos-relative m-b-30">
					

						<div class="block1-wrapbtn w-size2">
						
						</div>
					</div>
				</div>

	

				<div class="col-sm-10 col-md-8 col-lg-4 m-l-r-auto">
				
					<div class="block1 hov-img-zoom pos-relative m-b-30">
						<img src="~/img/thunder.jpeg" alt="IMG-BENNER">

						<div class="block1-wrapbtn w-size2">
						
							<a href="/trucks" class="flex-c-m size2 m-text2 bg3 hov1 trans-0-4">
							Trucks
							</a>
						</div>
					</div>

				
				</div>


			<div class="col-sm-10 col-md-8 col-lg-4 m-l-r-auto">
					
					<div class="block1 hov-img-zoom pos-relative m-b-30">
						<img src="~/img/spit_tabs.jpeg" alt="IMG-BENNER">

						<div class="block1-wrapbtn w-size2">
							
							<a href="/wheels" class="flex-c-m size2 m-text2 bg3 hov1 trans-0-4">
								Wheels
							</a>
						</div>
					</div>

				
					<div class="block1 hov-img-zoom pos-relative m-b-30">
					

						<div class="block1-wrapbtn w-size2">
							
						</div>
					</div>
				</div>


			</div>
		</div>
	</section>


	<section class="newproduct bgwhite p-t-45 p-b-105">
		<div class="container">
			<div class="sec-title p-b-60">
				<h3 class="m-text5 t-center">
					Featured Products
				</h3>
			</div>

			
			<div class="wrap-slick2">
				<div class="slick2">

					<div class="item-slick2 p-l-15 p-r-15">
					
						<div class="block2">
							<div class="block2-img wrap-pic-w of-hidden pos-relative block2-labelnew">
								<img src="@ViewBag.Prod1.Photo" alt="IMG-PRODUCT">

								<div class="block2-overlay trans-0-4">
						
									<div class="block2-btn-addcart w-size1 trans-0-4">
									
                    <form style="display: inline" action="/addcart/@ViewBag.Prod1.ProductId" method="get">
                        <button class="btn btn-success">Add to Cart</button>
                      </form>



									
									</div>
								</div>
							</div>

							<div class="block2-txt p-t-20">
								
								@ViewBag.Prod1.Name
							

								<span class="block2-price m-text6 p-r-5">
									$@ViewBag.Prod1.Price
								</span>
							</div>
						</div>
					</div>

					<div class="item-slick2 p-l-15 p-r-15">
					
						<div class="block2">
							<div class="block2-img wrap-pic-w of-hidden pos-relative">
								<img src="@ViewBag.Prod2.Photo" alt="IMG-PRODUCT">

								<div class="block2-overlay trans-0-4">
						

									<div class="block2-btn-addcart w-size1 trans-0-4">
									
										 <form style="display: inline" action="/addcart/@ViewBag.Prod2.ProductId" method="get">
                        <button class="btn btn-success">Add to Cart</button>
                      </form>
									</div>
								</div>
							</div>

							<div class="block2-txt p-t-20">
						
									@ViewBag.Prod2.Name
							

								<span class="block2-price m-text6 p-r-5">
									$@ViewBag.Prod2.Price
								</span>
							</div>
						</div>
					</div>

					<div class="item-slick2 p-l-15 p-r-15">
					
						<div class="block2">
							<div class="block2-img wrap-pic-w of-hidden pos-relative">
								<img src="@ViewBag.Prod3.Photo" alt="IMG-PRODUCT">

								<div class="block2-overlay trans-0-4">


									<div class="block2-btn-addcart w-size1 trans-0-4">
									
									 <form style="display: inline" action="/addcart/@ViewBag.Prod3.ProductId" method="get">
                        <button class="btn btn-success">Add to Cart</button>
                      </form>
									</div>
								</div>
							</div>

							<div class="block2-txt p-t-20">
								
									@ViewBag.Prod3.Name
								

								<span class="block2-price m-text6 p-r-5">
									$@ViewBag.Prod3.Price
								</span>
							</div>
						</div>
					</div>

					<div class="item-slick2 p-l-15 p-r-15">
					
						<div class="block2">
							<div class="block2-img wrap-pic-w of-hidden pos-relative block2-labelsale">
								<img src="@ViewBag.Prod4.Photo" alt="IMG-PRODUCT">

								<div class="block2-overlay trans-0-4">


									<div class="block2-btn-addcart w-size1 trans-0-4">
									
									 <form style="display: inline" action="/addcart/@ViewBag.Prod4.ProductId" method="get">
                        <button class="btn btn-success">Add to Cart</button>
                      </form>
									</div>
								</div>
							</div>

							<div class="block2-txt p-t-20">
							
									@ViewBag.Prod4.Name
							

								<span class="block2-oldprice m-text7 p-r-5">
									$@ViewBag.Prod4.Price
								</span>

								<span class="block2-newprice m-text8 p-r-5">
									$@sale
								</span>
							</div>
						</div>
					</div>

				
						</div>
					</div>
				</div>
			</div>

		</div>
	</section>

	
	<section class="shipping bgwhite p-t-62 p-b-46">
		<div class="flex-w p-l-15 p-r-15">
			<div class="flex-col-c w-size5 p-l-15 p-r-15 p-t-16 p-b-15 respon1">
				<h4 class="m-text12 t-center">
					Free Delivery Worldwide
				</h4>

				<a href="/" class="s-text11 t-center">
					Click here for more info
				</a>
			</div>

			<div class="flex-col-c w-size5 p-l-15 p-r-15 p-t-16 p-b-15 bo2 respon2">
				<h4 class="m-text12 t-center">
					30 Days Return
				</h4>

				<span class="s-text11 t-center">
					Simply return it within 30 days for an exchange.
				</span>
			</div>

			<div class="flex-col-c w-size5 p-l-15 p-r-15 p-t-16 p-b-15 respon1">
				<h4 class="m-text12 t-center">
					Store Hours
				</h4>

				<span class="s-text11 t-center">
					Monday to Sunday 10am-8pm PST
				</span>
			</div>
		</div>
	</section>


	

